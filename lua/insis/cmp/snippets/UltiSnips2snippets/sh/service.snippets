snippet service_simple "simple service"
[Unit]
Description=${1:Device Manager}
After=network.target

[Service]
Type=simple

Restart=on-failure
RestartSec=10

WorkingDirectory=${2:/home/gdgyy}
ExecStart=${3:/home/gopath/bin/devicemanager -c config.json}

[Install]
WantedBy=multi-user.target
endsnippet
